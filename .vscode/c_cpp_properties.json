{
    "configurations": [
        {
            "name": "GBDK",
            "includePath": [
                "${env:GBDK_HOME}/include",
                "${workspaceFolder}/src",
                "${workspaceFolder}/res",
                "${workspaceFolder}/src/lib/include"
            ],
            "browse": {
                "path": [
                    "${env:GBDK_HOME}/include",
                    "${workspaceFolder}/src"
                ],
                "limitSymbolsToIncludedHeaders": true
            },
            "defines": [
                "__PORT_sm83",
                "__TARGET_gb",
                "__GBC__"
            ],
            "compilerPath": "${env:GBDK_HOME}/bin/lcc",
            "cStandard": "c99",
            "intelliSenseMode": "gcc-x86"
        }
    ],
    "version": 4
}